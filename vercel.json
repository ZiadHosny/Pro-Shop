{
    "version": 2,
    "builds": [
        {
            "src": "./backend/src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "backend/src/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/src/index.js"
        }
    ]
}